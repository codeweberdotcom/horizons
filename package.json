{
	"name": "horizons-theme",
	"version": "1.0.0",
	"description": "Horizons theme blocks",
	"scripts": {
		"build": "node scripts/build-block.js",
		"build:block-only": "wp-scripts build blocks/practice-categories-grid/index.js --output-path=blocks/practice-categories-grid --output-filename=index.js --config webpack.config.js",
		"start": "wp-scripts start blocks/practice-categories-grid/index.js --output-path=blocks/practice-categories-grid --output-filename=index.js --config webpack.config.js"
	},
	"devDependencies": {
		"@wordpress/scripts": "^25.4.0"
	},
	"dependencies": {
		"ajv": "^8.17.1"
	}
}
